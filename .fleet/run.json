{
  "configurations": [
    {
      "name": "Clean Project",
      "type": "gradle",
      "tasks": [
        "clean"
      ]
    },
    {
      "name": "Build Starter-Project-Ktor",
      "type": "gradle",
      "tasks": [
        ":classes"
      ],
      "initScripts": {
        "flmapper": "ext.mapPath = { path -> null }",
        "Build Starter-Project-Ktor": "System.setProperty('org.gradle.java.compile-classpath-packaging', 'true')\n"
      },
      "debugJvm": "true"
    },
    {
      "name": "Development",
      "type": "jps-run",
      "dependsOn": [
        "Build Starter-Project-Ktor"
      ],
      "mainClass": "com.ApplicationKt",
      "module": "Starter-Project-Ktor.main",
      "options": [
        "-Dfile.encoding=UTF-8",
        "-Dsun.stdout.encoding=UTF-8",
        "-Dsun.stderr.encoding=UTF-8"
      ]
    },
  ]
}